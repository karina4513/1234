from itertools import *
k=0
for x in product('012345678',repeat=7):
    s=''.join(x)
    if s[0]!='0' and all(int(c)%3==2 for c in s):
        f = 1
        for i in range(len(s) - 1):
            if (int(s[i]) % 2) == (int(s[i + 1]) % 2):
                f = 0
        if f == 1:
            k += 1
print(k)
