from itertools import *

def cc(n):

    s=[]
    while n>0:
        s=[n%12]+s
        n//=12
    return s

k=0
for x in range(10000,100000):
    s=cc(x)
    f=1
    for i in range(len(s)-1):
        if ((s[i])%2)==((s[i+1])%2) or s[i]%3!=2 or s[-1]%3!=2:
            f=0
    if f==1:
        k+=1
print(k)
